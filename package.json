{
  "name": "emilio-dev",
  "version": "0.0.1",
  "description": "emilio.dev",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "lint": "eslint \"src/**/*.{js,jsx,ts,tsx}\" \"*.{js,jsx,ts,tsx}\"",
    "format": "prettier --write \"*.{js,jsx,ts,tsx,json,md}\" \"src/**/*.{js,jsx,ts,tsx,json,md}\"",
    "build": "npm run :clean:dist && npm run :build:bundle && npm run :build:11ty",
    "watch": "npm run :clean:dist && npm run :parallel -- \"npm run :watch:bundle\" \"npm run :watch:11ty\"",
    "serve": "npm run :clean:dist && npm run :parallel -- \"npm run :serve:cms-proxy\" \"npm run :watch:bundle\" \"npm run :serve:11ty\"",
    "start": "npm run serve",
    ":parallel": "concurrently --raw --kill-others-on-fail",
    ":clean:dist": "rimraf dist",
    ":build:bundle": "webpack --progress",
    ":watch:bundle": "webpack --progress --watch",
    ":build:11ty": "eleventy",
    ":watch:11ty": "eleventy --watch",
    ":serve:11ty": "eleventy --serve --port 5000",
    ":serve:cms-proxy": "netlify-cms-proxy-server"
  },
  "dependencies": {
    "@babel/runtime": "7.16.5",
    "netlify-cms-app": "2.15.61"
  },
  "devDependencies": {
    "@11ty/eleventy": "0.12.1",
    "@babel/core": "7.16.5",
    "@babel/plugin-proposal-class-properties": "7.16.5",
    "@babel/plugin-proposal-decorators": "7.16.5",
    "@babel/plugin-transform-runtime": "7.16.5",
    "@babel/preset-env": "7.16.5",
    "@babel/preset-typescript": "7.16.5",
    "@typescript-eslint/eslint-plugin": "5.7.0",
    "@typescript-eslint/parser": "5.7.0",
    "babel-loader": "8.2.3",
    "babel-plugin-const-enum": "1.2.0",
    "clean-webpack-plugin": "4.0.0",
    "concurrently": "6.4.0",
    "css-loader": "6.5.1",
    "eslint": "8.4.1",
    "eslint-plugin-react": "7.27.1",
    "fs-extra": "10.0.0",
    "html-minifier": "4.0.0",
    "mini-css-extract-plugin": "2.4.5",
    "netlify-cli": "8.1.3",
    "netlify-cms-proxy-server": "1.3.22",
    "optimize-css-assets-webpack-plugin": "6.0.1",
    "postcss": "8.4.5",
    "postcss-loader": "6.2.1",
    "postcss-preset-env": "7.0.1",
    "prettier": "2.5.1",
    "rimraf": "3.0.2",
    "sass": "1.45.0",
    "sass-loader": "12.4.0",
    "style-loader": "3.3.1",
    "terser-webpack-plugin": "5.2.5",
    "webpack": "5.76.0",
    "webpack-cli": "4.9.1",
    "webpack-fix-style-only-entries": "0.6.1",
    "webpack-manifest-plugin": "4.0.2"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emilio-martinez/emilio.dev.git"
  },
  "author": "Emilio Martinez <e.marticor@gmail.com>",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/emilio-martinez/emilio.dev/issues"
  },
  "homepage": "https://github.com/emilio-martinez/emilio.dev#readme"
}
