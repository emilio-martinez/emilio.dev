{
  "name": "emilio-dev",
  "version": "0.0.1",
  "description": "emilio.dev",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "format": "prettier --write \"*.{js,jsx,json,md}\" \"src/**/*.{js,jsx,json,md}\"",
    "build": "npm run :clean:dist && npm run :build:bundle && npm run :build:11ty",
    "watch": "npm run :clean:dist && npm run :parallel -- \"npm run :watch:bundle\" \"npm run :watch:11ty\"",
    "serve": "npm run :clean:dist && npm run :parallel -- \"npm run :serve:cms-proxy\" \"npm run :watch:bundle\" \"npm run :serve:11ty\"",
    "start": "npm run serve",
    ":parallel": "concurrently --raw --kill-others-on-fail",
    ":clean:dist": "rimraf dist",
    ":build:bundle": "webpack --progress",
    ":watch:bundle": "webpack --progress --watch",
    ":build:11ty": "eleventy",
    ":watch:11ty": "eleventy --watch",
    ":serve:11ty": "eleventy --serve --port 5000",
    ":serve:cms-proxy": "netlify-cms-proxy-server"
  },
  "dependencies": {
    "netlify-cms-app": "2.12.19",
    "preact": "10.4.8"
  },
  "devDependencies": {
    "@11ty/eleventy": "0.11.0",
    "concurrently": "5.3.0",
    "netlify-cli": "2.59.3",
    "netlify-cms-proxy-server": "1.3.2",
    "prettier": "2.1.1",
    "rimraf": "3.0.2",
    "webpack": "4.44.1",
    "webpack-cli": "3.3.12",
    "webpack-manifest-plugin": "2.2.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emilio-martinez/emilio.dev.git"
  },
  "author": "Emilio Martinez <e.marticor@gmail.com>",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/emilio-martinez/emilio.dev/issues"
  },
  "homepage": "https://github.com/emilio-martinez/emilio.dev#readme"
}
