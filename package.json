  "description": "emilio.dev",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "build": "concurrently --kill-others-on-fail \"rm -rf dist\" \"rollup -c\" \"eleventy\"",
    "serve": "concurrently --kill-others-on-fail \"rm -rf dist\" \"netlify-cms-proxy-server\" \"rollup -c --watch\" \"eleventy --serve --port 5000\"",
