    "@rollup/plugin-node-resolve": "9.0.0",
    "concurrently": "5.3.0",
    "netlify-cli": "^2.59.2",
    "netlify-cms-proxy-server": "1.3.2",
    "rollup": "2.26.8",
    "rollup-plugin-terser": "7.0.1"
  },
