{
  "name": "emilio-dev",
  "version": "0.0.1",
  "description": "emilio.dev",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "prebuild": "rimraf dist",
    "build": "concurrently --kill-others-on-fail \"npm::build:rollup\" \"npm::build:11ty\"",
    "preserve": "rimraf dist",
    "serve": "concurrently --kill-others-on-fail \"npm::serve:cms-proxy\" \"npm::watch:rollup\" \"npm::serve:11ty\"",
    "prestart": "rimraf dist",
    "start": "concurrently --kill-others-on-fail \"npm::watch:rollup\" \"npm::watch:11ty\"",
    ":build:rollup": "rollup -c",
    ":watch:rollup": "rollup -c --watch",
    ":build:11ty": "eleventy",
    ":watch:11ty": "eleventy --watch",
    ":serve:11ty": "eleventy --serve --port 5000",
    ":serve:cms-proxy": "netlify-cms-proxy-server"
  },
  "dependencies": {
    "netlify-cms-app": "2.12.19",
    "preact": "10.4.8"
  },
  "devDependencies": {
    "@11ty/eleventy": "0.11.0",
    "@erquhart/rollup-plugin-node-builtins": "2.1.5",
    "@rollup/plugin-alias": "3.1.1",
    "@rollup/plugin-commonjs": "15.0.0",
    "@rollup/plugin-json": "4.1.0",
    "@rollup/plugin-node-resolve": "9.0.0",
    "@rollup/plugin-replace": "2.3.3",
    "concurrently": "5.3.0",
    "netlify-cli": "2.59.3",
    "netlify-cms-proxy-server": "1.3.2",
    "rimraf": "3.0.2",
    "rollup": "2.26.9",
    "rollup-plugin-styles": "3.10.0",
    "rollup-plugin-terser": "7.0.1",
    "rollup-plugin-visualizer": "4.1.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/emilio-martinez/emilio.dev.git"
  },
  "author": "Emilio Martinez <e.marticor@gmail.com>",
  "license": "UNLICENSED",
  "bugs": {
    "url": "https://github.com/emilio-martinez/emilio.dev/issues"
  },
  "homepage": "https://github.com/emilio-martinez/emilio.dev#readme"
}
